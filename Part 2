   "outputs": [],
   "source": [
    "import random\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def kubik(n: int) -> list:\n",
    "    \"\"\"\n",
    "    Генерирует список случайных подбрасываний кубика.\n",
    "\n",
    "    :param n: Количество подбрасываний\n",
    "    :return: Список случайных значений кубика\n",
    "    \"\"\"\n",
    "    data = []\n",
    "    for _ in range(n):\n",
    "        data.append(random.randint(1, 6))\n",
    "    return data\n",
    "\n",
    "def count_rate(kub_data: list):\n",
    "    \"\"\"\n",
    "    Возвращает частоту выпадания значений кубика.\n",
    "\n",
    "    :param kub_data: Данные эксперимента\n",
    "    :return: Словарь с частотой выпадания значений кубика\n",
    "    \"\"\"\n",
    "    kub_rate = {}\n",
    "    for i in kub_data:\n",
    "        if i in kub_rate:\n",
    "            kub_rate[i] += 1\n",
    "        else:\n",
    "            kub_rate[i] = 1\n",
    "    return kub_rate\n",
    "\n",
    "\n",
    "def sort_rate(counted_rate: dict):\n",
    "    \"\"\"\n",
    "    Возвращает отсортированную частоту по ключу.\n",
    "\n",
    "    :param counted_rate: Неотсортированная частота\n",
    "    :return: Отсортированный словарь с частотой\n",
    "    \"\"\"\n",
    "    sorted_rate = {}\n",
    "    for key in sorted(counted_rate.keys()):\n",
    "        sorted_rate[key] = counted_rate[key]\n",
    "    return sorted_rate\n",
    "\n",
    "def create_dataframe(sorted_rate: dict):\n",
    "    \"\"\"\n",
    "    Создает Pandas DataFrame из данных о частоте.\n",
    "\n",
    "    :param sorted_rate: Отсортированный словарь с частотой\n",
    "    :return: Pandas DataFrame\n",
    "    \"\"\"\n",
    "    df = pd.DataFrame(sorted_rate, index=[0])\n",
    "    df = df.T\n",
    "    df = df.rename(columns={0: 'Частота'})\n",
    "    df.insert(0, 'Количество выпаданий', range(1, 1 + len(df)))\n",
    "    return df\n",
    "\n",
    "def probability_solving(dataframe: pd.DataFrame):\n",
    "    \"\"\"\n",
    "    Вычисляет вероятность полученных результатов.\n",
    "\n",
    "    :param dataframe: DataFrame с частотой\n",
    "    :return: DataFrame с вероятностью\n",
    "    \"\"\"\n",
    "    sum_rate = dataframe['Частота'].sum()\n",
    "    probability = [i / sum_rate for i in dataframe['Частота']]\n",
    "    dataframe['Вероятность'] = probability\n",
    "    return dataframe\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "id": "2fcc7407-6688-4015-99b8-5e84370fdcb8",
