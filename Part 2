    "# Построение гистограмм\n",
    "fig, axes = plt.subplots(1, len(n_throws_list), figsize=(15, 5))\n",
    "colors = ['red', 'green', 'blue', 'purple']\n",
    "\n",
    "for i, n_throws in enumerate(n_throws_list):\n",
    "    axes[i].bar(results[i].index, results[i]['Вероятность'], color=colors[i], label=f'{n_throws} бросков')\n",
    "    axes[i].set_xlabel('Значение кубика')\n",
    "    axes[i].set_ylabel('Вероятность')\n",
    "    axes[i].set_title(f'{n_throws} бросков')\n",
    "    axes[i].legend()\n",
