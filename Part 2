      "1                     1     1690       0.1690\n",
      "2                     2     1606       0.1606\n",
      "3                     3     1630       0.1630\n",
      "4                     4     1721       0.1721\n",
      "5                     5     1671       0.1671\n",
      "6                     6     1682       0.1682\n",
      "------------------------------\n",
      "Результаты для 100000 бросков:\n",
      "   Количество выпаданий  Частота  Вероятность\n",
      "1                     1    16517      0.16517\n",
      "2                     2    16653      0.16653\n",
      "3                     3    16769      0.16769\n",
      "4                     4    16637      0.16637\n",
      "5                     5    16648      0.16648\n",
      "6                     6    16776      0.16776\n",
      "------------------------------\n"
     ]
    }
   ],
   "source": [
    "results = []\n",
    "\n",
    "# Броски кубика для различных количеств бросков\n",
    "n_throws_list = [100, 1000, 10000, 100000]\n",
    "for i, n_throws in enumerate(n_throws_list):\n",
    "    kub_data = kubik(n_throws)\n",
    "    kub_rate = count_rate(kub_data)\n",
    "    sorted_rate = sort_rate(kub_rate)\n",
    "    df = create_dataframe(sorted_rate)\n",
    "    proba = probability_solving(df)\n",
    "    results.append(proba)\n",
    "\n",
    "# Вывод результатов\n",
    "for i, proba in enumerate(results):\n",
    "    print(f\"Результаты для {n_throws_list[i]} бросков:\")\n",
    "    print(proba)\n",
    "    print(\"-\" * 30)"
   ]
  },
